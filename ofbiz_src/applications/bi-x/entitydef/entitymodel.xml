<?xml version="1.0" encoding="UTF-8"?>

<entitymodel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entitymodel.xsd">

    <entity entity-name="CurrencyValue" package-name="org.ofbiz.bi.dimension" title="Currency Dimension">
        <description>Currency dimension. The natural key is [currencyId]</description>
        <field name="currencyId" type="id-ne"></field>
        <field name="currencyIdTo" type="id-ne"></field>
        <field name="date" type="id-ne"></field>
        <field name="conversionFactor" type="floating-point"></field>
        <prim-key field="currencyId"/>
        <prim-key field="currencyIdTo"/>
        <prim-key field="date"/>
    </entity>
    
    <entity entity-name="SchedulePentaho" package-name="org.ofbiz.bi.job">
    	<field name="scheduleId" type="id-ne"/>
    	<field name="job" type="name"/>
    	<field name="date" type="date-time"/>
    	<prim-key field="scheduleId"/>
    </entity>
    
</entitymodel>
